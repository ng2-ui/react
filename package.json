{
  "name": "@ngui/react",
  "version": "0.4.1",
  "description": "Angular2 React Component",
  "license": "MIT",
  "main": "dist/react.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --quiet --port 9001 --content-base app --config app/webpack.config --open",
    "lint": "tslint 'sr7/**/*.ts' 'app/**/*.ts'",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:ngc build:umd build:app",
    "build:ngc": "ngc -p tsconfig.ngc.json",
    "build:umd": "NODE_ENV=prod webpack",
    "build:app": "NODE_ENV=prod webpack --config app/webpack.config",
    "upgrade": "npm-check-updates --upgradeAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2-ui/react.git"
  },
  "author": "Allen Kim <allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/ng2-ui/react/issues"
  },
  "homepage": "https://github.com/ng2-ui/react#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/compiler-cli": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0",
    "@angular/http": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/platform-browser-dynamic": "^2.3.0",
    "@angular/router": "^3.3.0",
    "@ngui/utils": "^0.7.0",
    "@types/node": "^6.0.31",
    "@types/react": "0.0.0",
    "angular2-template-loader": "^0.6.0",
    "codelyzer": "^2.0.0-beta.3",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "css-raw-loader": "^0.1.1",
    "jsx-loader": "^0.13.2",
    "npm-check-updates": "^2.8.8",
    "npm-run-all": "^3.1.2",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-color": "^2.11.1",
    "react-day-picker": "^3.1.1",
    "react-dom": "^15.4.2",
    "react-highcharts": "^11.5.0",
    "react-openlayers": "^0.1.13",
    "react-select": "^1.0.0-rc.2",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-rc.5",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.2",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "zone.js": "^0.7.2"
  }
}
